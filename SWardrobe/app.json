{
  "expo": {
    "name": "SWardrobe",
    "slug": "SWardrobe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "permissions": [
      "CAMERA"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kov1103.SWardrobe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "dai-hoc-bach-khoa-dhqg-tphcm"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d510510b-e865-4dc6-b8d5-4c63065755e8"
      }
    }
  }
}
